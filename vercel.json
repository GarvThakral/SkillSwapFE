{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
      "routes": [
        {
          "src": "/assets/(.*)",
          "headers": {
            "cache-control": "public, max-age=31536000, immutable"
          }
        }
      ],
      "build": {
        "env": {
          "NODE_OPTIONS": "--max-old-space-size=4096"
        }
      }
    
  }
  